{
    "$id": "tables",
    "$root": true,
    "type": "object",
    "dynamicDefaults": {
        "id": "uuid",
        "ts_created": "datetime"
    },
    "allOf": [
        {
            "$ref": "core"
        },
        {
            "properties": {
                "version": {
                    "type": "string",
                    "maxLength": 11
                },
                "schema_name": {
                    "type": "string",
                    "maxLength": 30,
                    "minLength": 1
                },
                "name": {
                    "type": "string",
                    "maxLength": 30,
                    "minLength": 1
                },
                "db_enabled": {
                    "type": "boolean",
                    "nullable": false,
                    "default": true
                },
                "es_enabled": {
                    "type": "boolean",
                    "nullable": false,
                    "default": false
                },
                "cache_enabled": {
                    "type": "boolean",
                    "nullable": false,
                    "default": false
                },
                "schema": {
                    "type": "object"
                },
                "hash": {
                    "type": "string",
                    "maxLength": 64
                }
            }
        }
    ]
}