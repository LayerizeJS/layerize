{
  "name": "layerize",
  "version": "0.9.1",
  "description": "A framework to mange stacked data layers like postgresql, redis and elasticsearch.",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --exit",
    "test-coverage": "nyc npm test && nyc report --reporter=html",
    "test-travis": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "test-debug": "DEBUG=layerize* mocha",
    "commit": "npx git-cz",
    "commitmsg": "validate-commit-msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "eslint \"./**/*.js\"",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LayerizeJS/layerize.git"
  },
  "author": "Jason Futch @jasonfutch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LayerizeJS/layerize/issues"
  },
  "homepage": "https://layerizejs.github.io/",
  "devDependencies": {
    "@enspire/eslint-config-enspire": "^1.1.1",
    "coveralls": "^3.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "docdash": "^1.1.0",
    "eslint": "^5.16.0",
    "husky": "^1.3.1",
    "jsdoc": "^3.6.3",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-prepare": "^0.1.0",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "ajv-async": "^1.0.1",
    "cryptr": "^4.0.2",
    "debug": "^4.1.1",
    "deep-diff": "^1.0.2",
    "dynamodb": "git+https://git@github.com/LayerizeJS/dynamodb.git",
    "elasticsearch": "^15.4.1",
    "extend": "^3.0.2",
    "glob": "^7.1.3",
    "ioredis": "^4.9.0",
    "is-buffer": "^2.0.3",
    "joi": "^14.3.1",
    "knex": "^0.16.5",
    "lodash": "^4.17.15",
    "nested-property": "0.0.7",
    "node-object-hash": "^1.4.2",
    "notation": "^1.3.6",
    "nyc": "^14.0.0",
    "object-sizeof": "^1.3.0",
    "papaparse": "^4.6.3",
    "pg": "^7.10.0",
    "redis-eventemitter": "^2.0.1",
    "redis-lock": "^0.1.4",
    "role-acl": "^3.3.4",
    "rsmq": "^0.9.3",
    "unset-value": "^1.0.0",
    "util": "^0.11.1",
    "uuid": "^3.3.2"
  }
}
